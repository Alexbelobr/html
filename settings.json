{
  "prettier.jsxSingleQuote": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
    "printWidth": 80,
  "useTabs": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBrackeSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always"
}
